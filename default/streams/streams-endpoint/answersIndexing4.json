{
  "$id": "answersIndexing4",
  "$schema": "https://schema.yext.com/config/streams/streams-endpoint/v1",
  "name": "Streams Endpoint",
  "stream": {
    "source": "knowledgeGraph",
    "fields": [
      "uid",
      "id",
      "timezone",
      "meta",
      "savedFilters",
      "displayCoordinate",
      "yextDisplayCoordinate",
      "geocodedCoordinate",
      "cityCoordinate",
      "addressHidden",
      "name",
      "keywords"
    ],
    "filter": {
      "entityTypes": [
        "atm"
      ]
    },
    "localization": {
      "locales": [
        "en"
      ],
      "excludePrimary": true
    }
  },
  "fieldIndexes": [
    {
      "field": "id"
    }
  ]
}
