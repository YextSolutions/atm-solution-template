{
  "$id": "c_Top_Search_Terms57477",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Top Search Terms",
  "description": "Answers Searches by Search Term",
  "query": {
    "metrics": [
      "yext/ANSWERS_SEARCHES"
    ],
    "dimensions": [
      "yext/ANSWERS_SEARCH_TERM"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "DAYS_14"
      },
      {
        "filter": "yext/ANSWERS_EXPERIENCE",
        "in": [
          "atm-answers"
        ]
      }
    ]
  },
  "display": {
    "table": {
      "sort": {
        "column": {
          "metric": "yext/ANSWERS_SEARCHES"
        },
        "descending": true
      }
    }
  }
}