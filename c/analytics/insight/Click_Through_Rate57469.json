{
  "$id": "c_Click_Through_Rate57469",
  "$schema": "https://schema.yext.com/config/analytics/insight/v1",
  "name": "Click Through Rate",
  "description": "Answers Click Through Rate by Answers Experience",
  "query": {
    "metrics": [
      "yext/ANSWERS_CLICK_THROUGH_RATE"
    ],
    "dimensions": [
      "yext/ANSWERS_EXPERIENCE"
    ],
    "filters": [
      {
        "filter": "yext/date",
        "in": "DAYS_14"
      },
      {
        "filter": "yext/ANSWERS_EXPERIENCE",
        "in": [
          "atm-answers"
        ]
      }
    ]
  },
  "display": {
    "hero": {
      "aggregation": "SUM"
    }
  }
}